{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "media_presences",
  "info": {
    "name": "MediaPresence",
    "description": "Presencia en los medios",
    "mainField": "Slug"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Title": {
      "type": "text",
      "required": true
    },
    "Content": {
      "default": "Texto de la noticia",
      "required": true,
      "type": "richtext"
    },
    "MainImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Source": {
      "required": true,
      "type": "string"
    },
    "SourceLink": {
      "type": "text",
      "required": true
    },
    "PublicationDate": {
      "required": true,
      "type": "date"
    },
    "Slug": {
      "type": "string",
      "unique": true
    }
  }
}
