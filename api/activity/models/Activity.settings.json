{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "activities",
  "info": {
    "name": "activity",
    "description": "Actividades",
    "mainField": "Slug"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Title": {
      "required": true,
      "type": "text"
    },
    "Location": {
      "required": true,
      "type": "text"
    },
    "Description": {
      "required": true,
      "type": "richtext"
    },
    "MainImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Category": {
      "model": "activity-category"
    },
    "ActivityDateTime": {
      "type": "datetime"
    },
    "Slug": {
      "type": "string",
      "unique": true
    }
  }
}
