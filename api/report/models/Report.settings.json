{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "reports",
  "info": {
    "name": "report",
    "description": "Lo vamos a utilizar para  Registro Femicidios, Encuestas o cualquier informe que querramos compartir"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "required": true,
      "type": "string"
    },
    "mainImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "content": {
      "required": true,
      "type": "richtext"
    },
    "attachment": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "fromDate": {
      "type": "date",
      "required": true
    },
    "toDate": {
      "required": true,
      "type": "date"
    },
    "lawNumber": {
      "type": "string"
    },
    "additionalImages": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "category": {
      "model": "report-category",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
