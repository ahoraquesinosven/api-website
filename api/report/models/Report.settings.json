{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "reports",
  "info": {
    "name": "report",
    "description": "Lo vamos a utilizar para  Registro Femicidios, Encuestas o cualquier informe que querramos compartir",
    "mainField": "Slug"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Title": {
      "required": true,
      "type": "text"
    },
    "Summary": {
      "required": true,
      "type": "richtext"
    },
    "MainImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Attachment": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "FromDate": {
      "type": "date",
      "required": true
    },
    "ToDate": {
      "required": true,
      "type": "date"
    },
    "LawNumber": {
      "type": "string"
    },
    "AdditionalImages": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "Category": {
      "model": "report-category"
    },
    "Slug": {
      "type": "string",
      "unique": true
    }
  }
}
