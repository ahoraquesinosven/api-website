{
  "connection": "default",
  "collectionName": "mediapresences",
  "info": {
    "name": "mediapresence",
    "description": "Presencia en los medios"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "text",
      "required": true
    },
    "Content": {
      "default": "Texto de la noticia",
      "required": true,
      "type": "richtext"
    },
    "MainImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "Source": {
      "required": true,
      "type": "string"
    },
    "SourceLink": {
      "type": "text",
      "required": true
    },
    "PublicationDate": {
      "required": true,
      "type": "date"
    }
  }
}